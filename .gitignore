import java.time.LocalDate;

public class Customer {
    // Attributes
    private int customerId;
    private String customerName;
    private String accountNumber;
    private LocalDate dateOfBirth;
    private String bvn;
    private String phoneNumber;
    private String emailAddress;

    // Constructor
    public Customer(int customerId, String customerName, String accountNumber, LocalDate dateOfBirth, String bvn, String phoneNumber, String emailAddress) {
        this.customerId = customerId;
        this.customerName = customerName;
        this.accountNumber = accountNumber;
        this.dateOfBirth = dateOfBirth;
        this.bvn = bvn;
        this.phoneNumber = phoneNumber;
        this.emailAddress = emailAddress;
    }

    // Getter methods
    public int getCustomerId() {
        return customerId;
    }

    public String getCustomerName() {
        return customerName;
    }

    public String getAccountNumber() {
        return accountNumber;
    }

    public LocalDate getDateOfBirth() {
        return dateOfBirth;
    }

    public String getBvn() {
        return bvn;
    }

    public String getPhoneNumber() {
        return phoneNumber;
    }

    public String getEmailAddress() {
        return emailAddress;
    }

    // Main method for testing
    public static void main(String[] args) {
        // Create a customer object
        Customer customer = new Customer(1, "John Doe", "1234567890", LocalDate.of(1990, 5, 15), "12345678901234567", "123-456-7890", "john.doe@example.com");

        // Print customer details
        System.out.println("Customer ID: " + customer.getCustomerId());
        System.out.println("Customer Name: " + customer.getCustomerName());
        System.out.println("Account Number: " + customer.getAccountNumber());
        System.out.println("Date of Birth: " + customer.getDateOfBirth());
        System.out.println("BVN: " + customer.getBvn());
        System.out.println("Phone Number: " + customer.getPhoneNumber());
        System.out.println("Email Address: " + customer.getEmailAddress());
    }
}
